{
  "name": "gitcodeshare.com",
  "version": "0.1.0",
  "description": "https://contributhon.kr/project#gitshare",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all -p dev:front dev:backend",
    "dev:front": "node ./bin/front.js",
    "dev:backend": "nodemon --watch server -e ts,js ./bin/server.js",
    "predev": "npm run docker:start",
    "devstop": "npm run docker:stop",
    "docker:start": "node -r ts-node/register ./docker/dev_start.ts",
    "docker:stop": "node -r ts-node/register ./docker/dev_stop.ts",
    "build": "next build",
    "start": "npm-run-all -p prod:front prod:backend",
    "prod:front": "node ./bin/front.js",
    "prod:backend": "node ./bin/server.js",
    "precommit": "lint-staged && npm run contributors:build && git add README.md",
    "postinstall": "npm run build",
    "contributors:add": "all-contributors add",
    "contributors:build": "all-contributors generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsdevkr/gitCodeShare.com.git"
  },
  "author": "jsdev.kr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsdevkr/gitCodeShare.com/issues"
  },
  "homepage": "https://github.com/jsdevkr/gitCodeShare.com#readme",
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "prettier --parser flow --write",
        "git add"
      ],
      "*.{ts,tsx}": [
        "prettier --parser typescript --write",
        "tslint --fix",
        "git add"
      ]
    }
  },
  "dependencies": {
    "@types/chrome": "0.0.73",
    "@types/codemirror": "0.0.60",
    "@types/connect-redis": "^0.0.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/dockerode": "^2.5.5",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/http-proxy-middleware": "^0.17.5",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/morgan": "^1.7.35",
    "@types/next": "^6.1.4",
    "@types/node": "^10.9.4",
    "@types/passport": "^0.4.6",
    "@types/passport-github": "^1.1.3",
    "@types/puppeteer": "^1.6.3",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/redis": "^2.8.6",
    "@types/request": "^2.47.1",
    "@types/uuid": "^3.4.4",
    "@zeit/next-css": "^0.2.0",
    "@zeit/next-less": "^0.3.0",
    "@zeit/next-sass": "^0.2.0",
    "@zeit/next-typescript": "^1.1.0",
    "antd": "^3.9.1",
    "body-parser": "^1.18.3",
    "codemirror": "^5.40.0",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "http-proxy-middleware": "^0.19.0",
    "isomorphic-unfetch": "^2.1.1",
    "less": "^3.8.1",
    "mobx": "^5.1.0",
    "mobx-react": "^5.2.6",
    "mobx-state-tree": "^3.3.0",
    "morgan": "^1.9.0",
    "next": "^6.1.2",
    "next-compose-plugins": "^2.1.1",
    "node-sass": "^4.9.3",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "puppeteer": "^1.8.0",
    "react": "^16.5.0",
    "react-animations": "^1.0.0",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.5.0",
    "redis": "^2.8.0",
    "styled-components": "^3.4.5",
    "styled-normalize": "^8.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "all-contributors-cli": "^5.4.0",
    "babel-plugin-styled-components": "^1.6.3",
    "babel-plugin-transform-define": "^1.3.0",
    "docker-compose": "^0.8.1",
    "dockerode": "^2.5.6",
    "husky": "^0.14.3",
    "kill-port": "^1.3.1",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript-styled-plugin": "^0.10.0"
  }
}
